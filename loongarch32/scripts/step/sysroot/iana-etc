source env/sysroot/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=sysroot
export STEP_PACKAGENAME=iana-etc
export PACKAGE_VERSION=20250114
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/sysroot/iana-etc/20250114/
rm -rf ${BUILD_DIRECTORY}/iana-etc-20250114
tar xvf ${DOWNLOADDIR}/iana-etc-20250114.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/iana-etc-20250114
	cp -v services protocols ${SYSROOT_DIR}/etc
popd 
rm -rf ${BUILD_DIRECTORY}/iana-etc-20250114
